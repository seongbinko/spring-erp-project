package kr.co.jhta.erp.form;

import java.util.Arrays;

public class OrderRegisterForm {

	private int no;
	private int storeNo;
	private int employeeNo;
	private int[] productNos;
	private int[] amounts;
	
	public OrderRegisterForm() {}

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public int getStoreNo() {
		return storeNo;
	}

	public void setStoreNo(int storeNo) {
		this.storeNo = storeNo;
	}

	public int getEmployeeNo() {
		return employeeNo;
	}

	public void setEmployeeNo(int employeeNo) {
		this.employeeNo = employeeNo;
	}

	public int[] getProductNos() {
		return productNos;
	}

	public void setProductNos(int[] productNos) {
		this.productNos = productNos;
	}

	public int[] getAmounts() {
		return amounts;
	}

	public void setAmounts(int[] amounts) {
		this.amounts = amounts;
	}

	@Override
	public String toString() {
		return "OrderRegisterForm [no=" + no + ", storeNo=" + storeNo + ", employeeNo=" + employeeNo + ", productNos="
				+ Arrays.toString(productNos) + ", amounts=" + Arrays.toString(amounts) + "]";
	}

}