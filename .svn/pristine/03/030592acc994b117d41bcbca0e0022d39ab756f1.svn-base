package kr.co.jhta.erp.dto;

import org.apache.ibatis.type.Alias;

@Alias("TotalStatusByGradeDto")
public class TotalStatusByGradeDto {
	private Integer director;
	private Integer manager;
	private Integer subManager;
	private Integer seniorAssistant;
	private Integer assistant;
	private Integer staff;
	private Integer intern;
	private Integer totalStatus;
	
	public TotalStatusByGradeDto() {}

	public Integer getDirector() {
		return director;
	}

	public void setDirector(Integer director) {
		this.director = director;
	}

	public Integer getManager() {
		return manager;
	}

	public void setManager(Integer manager) {
		this.manager = manager;
	}

	public Integer getSubManager() {
		return subManager;
	}

	public void setSubManager(Integer subManager) {
		this.subManager = subManager;
	}

	public Integer getAssistant() {
		return assistant;
	}

	public void setAssistant(Integer assistant) {
		this.assistant = assistant;
	}

	public Integer getStaff() {
		return staff;
	}

	public void setStaff(Integer staff) {
		this.staff = staff;
	}

	public Integer getIntern() {
		return intern;
	}

	public void setIntern(Integer intern) {
		this.intern = intern;
	}

	public Integer getTotalStatus() {
		return totalStatus;
	}

	public void setTotalStatus(Integer totalStatus) {
		this.totalStatus = totalStatus;
	}

	public Integer getSeniorAssistant() {
		return seniorAssistant;
	}

	public void setSeniorAssistant(Integer seniorAssistant) {
		this.seniorAssistant = seniorAssistant;
	}

	@Override
	public String toString() {
		return "TotalStatusByGradeDto [director=" + director + ", manager=" + manager + ", subManager=" + subManager
				+ ", seniorAssistant=" + seniorAssistant + ", assistant=" + assistant + ", staff=" + staff + ", intern="
				+ intern + ", totalStatus=" + totalStatus + "]";
	}
}
